{
  "manifest_version": 2,

  "name": "CryptoDraw",
  "author": "Lining Wang",
  "description": "uploading, storing, and recopying creations",
  "version": "0.1",
  "version_name": "halcyon",
  // "default_locale": "en",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "CryptoDraw notifications"
  },

  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}